                                                <li><a href="#">- Dropdown Item</a></li>
                                                <li>
                                                    <a href="#">- Dropdown Item</a>
                                                    <ul class="dropdown">
                                                        <li><a href="#">- Even Dropdown</a></li>
                                                        <li><a href="#">- Even Dropdown</a></li>
                                                        <li><a href="#">- Even Dropdown</a></li>
                                                        <li><a href="#">- Even Dropdown</a></li>
                                                    </ul>
                                                </li>
                                                <li><a href="#">- Dropdown Item</a></li>
                                                <li><a href="#">- Dropdown Item</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </li>
                                <li>@Html.ActionLink("Project", "Index", "Projects", new { area = "User" },null)</li>
                                <li>@Html.ActionLink("Models","Index","Models", new { area = "User" }, null)</li>
                                <li>@Html.ActionLink("Casting", "Index", "Casting", new { area = "User" }, null)</li>
                                <li>
                                    <a href="#">Blog</a>
                                    <ul class="dropdown">
                                        <li>@Html.ActionLink("Blog","Index","Blog",new { area= "User"},null)</li>
                                        <li>@Html.ActionLink("Blogs Details", "Blogs", "Blog", new { area = "User" }, null)</li>
                                    </ul>
                                </li>
                                <li>@Html.ActionLink("Contacts", "Index", "Contact", new { area = "User" }, null)</li>
                            </ul>

                            <!-- Search Icon -->
                            <div class="search-icon" data-toggle="modal" data-target="#searchModal">
                                <i class="icon_search"></i>
                            </div>
                        </div>
                        <!-- Nav End -->

                    </div>
                </nav>
            </div>
        </div>
